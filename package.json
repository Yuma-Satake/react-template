  "scripts": {
    "dev": "vite --mode dev --host",
    "build": "pnpm build:dev",
    "build:dev": "tsc && vite build --mode dev",
    "build:analyze": "tsc && vite build --mode dev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc --noEmit",
